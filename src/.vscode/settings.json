{
	"files.exclude": {
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	"commentTranslate.targetLanguage": "zh-CN",
	"psi-header.templates": [
		{
			"language": "java",
			"template": [
				"@project chocolateFactory",
				"@classname <<filenamebase>>",
				"@description TODO",
				"@author <<NameWuu>>",
				"@date <<dateformat('YYYY-MM-DD HH:mm:ss')>>",
			],
		}
	],
	"psi-header.changes-tracking": {
		"isActive": true,
		"modDate": "@date ",
		"include": [],
		"autoHeader": "autoSave"
	},
	"psi-header.variables": [
		[
			"NameWuu",
			"Ngae Zeh-ghau"
		]
	],
	"psi-header.lang-config": [
		{
			"language": "java",
			"begin": "/**",
			"prefix": " * ",
			"end": " */",
		}
	],
	"java.refactor.renameFromFileExplorer": "autoApply",
}